function plotAll()
    plotFile('../results/PositionCircle-Cellular', ['Rx1'; 'Rx2'; 'Rx3']);
    plotFile('../results/PositionCircle-D2D-MaxDatarate', ['Rx1'; 'Rx2'; 'Rx3']);
    plotFile('../results/PositionCircle-D2D-ProportionalFair', ['Rx1'; 'Rx2'; 'Rx3']);
    plotFile('../results/PositionCircle-D2D-RoundRobin', ['Rx1'; 'Rx2'; 'Rx3']);

    plotFile('../results/PositionOptimal-Cellular', ['Rx1'; 'Rx2']);
    plotFile('../results/PositionOptimal-D2D-MaxDatarate', ['Rx1'; 'Rx2']);
    plotFile('../results/PositionOptimal-D2D-ProportionalFair', ['Rx1'; 'Rx2']);
    plotFile('../results/PositionOptimal-D2D-RoundRobin', ['Rx1'; 'Rx2']);